portfolio/
    ├── app.py                # Main app entry point
    ├── index.py              # Main layout and routing
    ├── assets/               # Global CSS/JS and assets
    │   ├── css/
    │   │   ├── global.css    # Global styles
    │   ├── js/
    │   │   ├── global.js     # Global scripts
    │   ├── images/           # Shared images
    │       ├── logo.png
    ├── static/               # Page-specific assets (CSS/JS/images)
    │   ├── css/
    │   │   ├── intro.css     # Intro page styles
    │   │   ├── about.css     # About Me styles
    │   ├── js/
    │   │   ├── intro.js      # Intro page scripts
    │   │   ├── about.js      # About Me scripts
    │   ├── images/
    │       ├── educational.png
    ├── db/                   # Database-related files
    │   ├── schema.sql        # Database schema
    ├── data/                 # JSON files for storing data
    │   ├── personal_info.json        # Personal information
    │   ├── education.json            # Educational data
    │   ├── society.json              # Society involvement data
    │   ├── projects.json             # Projects data
    │   ├── certificates.json         # Certificates data
    │   ├── skills.json               # Skills data
    │   ├── db_credentials.json       # db credentials
    ├── pages/                # Individual webpage layouts
    │   ├── intro.py          # Intro page
    │   ├── about_me.py       # About Me page
    │   ├── education.py      # Educational section
    │   ├── society.py        # Society Involvements
    │   ├── projects.py       # Projects section
    │   ├── certificates.py   # Certificates section
    │   ├── contact.py        # Contact Me page
    ├── utils/                # Utility scripts
    │   ├── db.py             # Database connection
    │   ├── json_utils.py     # JSON read/write utility functions
    │   ├── helpers.py        # Shared helper functions
    │   ├── clean_cache.py    # Delete .idea folders
    │   ├── fetch_data.py     # Script to update JSON files
    ├── requirements.txt      # Python dependencies
    ├── README.md             # Documentation
    └── .gitignore            # Ignore unnecessary files
